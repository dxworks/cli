{
  "name": "dxworks-cli",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/app/index.js",
  "bin": {
    "dxw": "./dist/app/index.js"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -a -u 1 src/assets/**/*.* dist",
    "start:voyenv:install": "cross-env NODE_ENV=dev nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app/index.ts voyenv install",
    "start:voyenv:instrument": "cross-env NODE_ENV=dev nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app/index.ts voyenv instrument",
    "start:dev:validate": "cross-env NODE_ENV=dev nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app/index.ts dev validate test.manifest.yml",
    "start:dev:validate-fix": "cross-env NODE_ENV=dev nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app/index.ts dev validate --fix test.manifest.yml",
    "run:dev:validate-fix": "cross-env NODE_ENV=dev node dist/app/index.js dev validate --fix test.manifest.yml",
    "create": "npm run build && npm run test",
    "build": "npm run clean && tsc -p . && npm run copy-files",
    "local": "npm run build && npm i -g && dxw -v",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "keywords": [],
  "author": "Mario Rivis",
  "license": "MIT",
  "devDependencies": {
    "@types/cli-progress": "^3.9.2",
    "@types/didyoumean": "^1.2.0",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.1.3",
    "@types/inquirer-autocomplete-prompt": "^1.3.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/request": "^2.48.7",
    "@types/semver": "^7.3.8",
    "@types/spdx-correct": "^2.0.0",
    "@types/string-template": "^1.0.2",
    "@types/unzipper": "^0.10.4",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "lerna": "^4.0.0",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/slug": "^5.0.2",
    "axios": "^0.22.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.9.1",
    "commander": "^8.2.0",
    "didyoumean": "^1.2.2",
    "figlet": "^1.5.2",
    "i": "^0.3.7",
    "inquirer": "^8.2.0",
    "inquirer-search-list": "^1.2.6",
    "isomorphic-git": "^1.10.1",
    "npm": "^7.24.2",
    "octokit": "^1.6.2",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "semver": "^7.3.5",
    "slug": "^5.1.0",
    "spdx-correct": "^3.1.1",
    "spdx-license-ids": "^3.0.10",
    "string-template": "^1.0.0",
    "unzipper": "^0.10.11",
    "winston": "^3.3.3",
    "yaml": "^1.10.2"
  }
}
