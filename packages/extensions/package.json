{
  "name": "@org.dxworks/extensions",
  "version": "0.1.0",
  "description": "Typescript extension functions for different types. This package is supposed to bring some of the Kotlin extension functions to Typescript",
  "keywords": [
    "extensions",
    "extension-functions",
    "typescript",
    "kotlin"
  ],
  "author": "Mario Rivis <mario.rivis@gmail.com>",
  "homepage": "https://github.com/dxworks/cli#readme",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": ["src/index.ts", "typings/index.d.ts"],
  "typings": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxworks/cli.git"
  },
  "scripts": {
    "clean": "rimraf dist/ *.tsbuildinfo",
    "create": "npm run build && npm run test",
    "build": "npm run clean && tsc -p .",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test": "jest --passWithNoTests",
    "test:dev": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/dxworks/cli/issues"
  }
}
